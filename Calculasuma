#include <iostream>
using namespace std;
int main()
{
    float numero1, numero2, suma;
    cout<<"Introducir primer numero: ";
    cin>> numero1;
    cout<< "Introducir segundo numero: ";
    cin>> numero2;
    suma=numero1+numero2;
    cout<<"El resultado de la suma es: "<< suma;
    

    return 0;
}
