#include <iostream>
using namespace std;
int main()
{
    float numero1, numero2, promedio;
   cout<<"Ingresar primer numero: ";
   cin>>numero1;
   cout<<"Ingresar segundo numero: ";
   cin>>numero2;
   promedio=numero1+numero2;
   cout<<"Ingresar tercer numero: ";
   cin>>numero1; 
   cout<<"Ingresar cuarto numero: ";
   cin>>numero2;
   promedio=promedio+numero1+numero2;
   cout<<"Ingresar quinto numero: ";
   cin>>numero1;
   promedio=(promedio+numero1)/5;
   cout<<"El promedio es: "<<promedio;

    return 0;
}
