#include <iostream>
#include <string>

using namespace std;

int main() {
    string cadena;
    cout<<"Ingresar cadena de oracion: ";
    getline(cin, cadena); 

   
    for (int i = 0; i < cadena.length(); i++) {
        cadena[i] = toupper(cadena[i]);
    }

    cout << cadena << endl;

    return 0;
}
